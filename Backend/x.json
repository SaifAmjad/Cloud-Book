{
    "version": 2,
    "name": "music-album",
    "builds": [
       { "src": "./main.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)",
        "dest": "./main.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
     }
    ]
}